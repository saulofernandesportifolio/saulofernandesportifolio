<?php

require_once('../fixa/bd.php');

$query = mysql_query("SELECT 
						reg_dt_entrada,	
						usuario,
						data_solicitacao,
						devolucao,
						canal_entrada,
						produto,
						servico,
						qtd_acessos,
						motivo_cancelamento,
						cnpj,
						razao_social,
						n_gestao_servicos,
						data_abertura_gestao,
						motivo_devolucao,
						area_solicitante,
						data_devolucao,
						data_encerramento,
						status_solicitacao
					FROM 
						v_geral_intragov
					WHERE reg_dt_entrada <> '' ");

while($fetch = mysql_fetch_array($query))
{
	$output[] = array (
			$fetch[0],  //  reg_dt_entrada,	
		    $fetch[1],	// usuario,
			 $fetch[2],	// data_solicitacao,
			$fetch[3],	// devolucao,
			$fetch[4],	// canal_entrada,
			$fetch[5],	// produto,
			$fetch[6],	// servico,
			$fetch[7],	// qtd_acessos,
			$fetch[8],	// motivo_cancelamento,
			$fetch[9],	// cnpj,
			$fetch[10],	// razao_social,
			$fetch[11],	// n_gestao_servicos,
			 $fetch[12],	// data_abertura_gestao,
			$fetch[13],	// motivo_devolucao,
			$fetch[14],	// area_solicitante,
			 $fetch[15],	// data_devolucao,
			 $fetch[16],	// data_encerramento,
			$fetch[17],	// status_solicitacao
	);
}

echo json_encode($output);

?>

